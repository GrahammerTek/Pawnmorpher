<?xml version="1.0" encoding="UTF-8" ?>
<Defs>
	
	<Pawnmorph.Social.PMInteractionWeightsDef>
		<defName>PMInteractionWeights_Raccoon</defName>
		<requiredMutationsAny>
			<li>EtherRaccoonJaw</li>
		</requiredMutationsAny>
		<!-- Muzzles provide 0.5 weight, ears 0.125 each (0.25 for both), tails 0.25, and morphs get an additional +1, for a total of 2. A fully mutated-morph is thus 4x as likely to chitter as a human with just a muzzle. A muzzled human will chitter half as often as chatting and a full morph will chitter twice as often as chatting -->
		<mutationWeights>
			<!-- Muzzles -->
			<li>
				<key>EtherRaccoonJaw</key>
				<value>0.5</value>
			</li>
			
			<!-- Tails -->
			<li>
				<key>EtherRaccoonTail</key>
				<value>0.25</value>
			</li>

			<!-- Ears -->
			<li>
				<key>EtherRaccoonEar</key>
				<value>0.125</value>
			</li>
		</mutationWeights>
		<morphWeights>
			<li>
				<key>RaccoonMorph</key>
				<value>1</value>
			</li>
		</morphWeights>
	</Pawnmorph.Social.PMInteractionWeightsDef>
	
	<!-- Chitchat -->
	<Pawnmorph.Social.PMInteractionDef ParentName="PMAnimalNoise_Chitchat">
		<defName>PMChitter_Chitchat</defName>
		<label>chitter</label>
		<symbol>Things/Mote/SpeechSymbols/Chitter</symbol>
		<initiatorThought>PawnmorphChitter</initiatorThought>
		<recipientThought>PawnmorphChitter</recipientThought>
		<logRulesInitiator>
			<rulesStrings>
				<li>r_logentry->[INITIATOR_nameDef] [talkedabout] [RECIPIENT_nameDef].</li>
				<li>r_logentry->[INITIATOR_nameDef] asked [RECIPIENT_nameDef] about [askedabout].</li>

				<li>talkedabout(p=4)->chittered at</li>
				<li>talkedabout->chittered to</li>
				<li>talkedabout->chirped at</li>

				<li>askedabout->cleaning [INITIATOR_possessive] hands</li>
				<li>askedabout->raccoons</li>
				<li>askedabout->crafting</li>
				<li>askedabout->night walks</li>
				<li>askedabout->trash</li>
				<li>askedabout->garbage</li>
				<li>askedabout->whiskers</li>
				<li>askedabout->striped tails</li>
				<li>askedabout->cleaning</li>
				<li>askedabout->something to chew on</li>
				<li>askedabout->gnawing habits</li>
				<li>askedabout->trees</li>
				<li>askedabout->leftovers</li>
				<li>askedabout->kibble</li>
				<li>askedabout->head pats</li>
