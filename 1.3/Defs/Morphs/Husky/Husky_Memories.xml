<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/ThoughtDef.xsd">
    <ThoughtDef ParentName="OutlookMemoryBase">
		<defName>HuskyTransformationMemory</defName>
        <stages>
            <li>
                <label>changed</label>
                <description>Honestly, this is not that bad...</description>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
            <li>
                <label>changed</label>
                <description>I'm so full of energy, I could run for days!</description>
                <baseMoodEffect>+3</baseMoodEffect>
            </li>
            <li>
                <label>changed</label>
                <description>I know I'm supposed to hate this but... I feel too excited to care.</description>
                <baseMoodEffect>-5</baseMoodEffect>
            </li>
            <li>
                <label>changed</label>
                <description>What's not to like? I guess it'd be fun to try the sled-pulling experience.</description>
                <baseMoodEffect>+1</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>

    <ThoughtDef ParentName="OutlookMemoryBase">
		<defName>HuskyRevertedMemory</defName>
        <stages>
            <li>
                <label>reverted</label>
                <description>Phew... I was having a bit too much fun.</description>
                <baseMoodEffect>3</baseMoodEffect>
            </li>
            <li>
                <label>reverted</label>
                <description>I'm not as peppy as I used to be.</description>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
            <li>
                <label>reverted</label>
                <description>I'm not a stinky dog anymore!</description>
                <baseMoodEffect>10</baseMoodEffect>
            </li>
            <li>
                <label>reverted</label>
                <description>No soft hugs for me.</description>
                <baseMoodEffect>-5</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>

	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherHuskyMuzzle</defName>
		<stages>
			<li>
				<label>Husky face</label>
				<description>My face is...actually rather cool.</description>
				<visible>true</visible>
				<baseMoodEffect>1</baseMoodEffect>
			</li>
			<li>
				<label>Husky face</label>
				<description>BARK BARK BARK.</description>
				<visible>true</visible>
				<baseMoodEffect>3</baseMoodEffect>
			</li>
			<li>
				<label>Husky face</label>
				<description>I shouldn't like it, but the husky is making me.</description>
				<visible>true</visible>
				<baseMoodEffect>0</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>
	
	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherHuskyEar</defName>
		<stages>
			<li>
				<label>Pointy dog ears</label>
				<description>They're pretty cute, I gotta say.</description>
				<visible>true</visible>
				<baseMoodEffect>1</baseMoodEffect>
			</li>
			<li>
				<label>Pointy dog ears</label>
				<description>They flick and point to sounds, and they're unreasonably fluffy!</description>
				<visible>true</visible>
				<baseMoodEffect>2</baseMoodEffect>
			</li>
			<li>
				<label>Pointy dog ears</label>
				<description>...sigh...they're OK I suppose...</description>
				<visible>true</visible>
				<baseMoodEffect>0</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>
	
	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherHuskyTail</defName>
		<stages>
			<li>
				<label>Curly fluff tail</label>
				<description>I gotta calm down, it keeps wagging...</description>
				<visible>true</visible>
				<baseMoodEffect>1</baseMoodEffect>
			</li>
			<li>
				<label>Curly fluff tail</label>
				<description>I'm so excited, it's wagging so fast!</description>
				<visible>true</visible>
				<baseMoodEffect>2</baseMoodEffect>
			</li>
			<li>
				<label>Curly fluff tail</label>
				<description>I know I don't like it...but it's still wagging and it kind of feels OK.</description>
				<visible>true</visible>
				<baseMoodEffect>0</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>
	
</Defs>
