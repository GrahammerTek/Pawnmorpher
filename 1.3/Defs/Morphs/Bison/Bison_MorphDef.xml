<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef ParentName="HerdMorph">
		<defName>BisonMorph</defName>
		<label>bisonmorph</label>
		<description>A large humanoid resulting when a human is exposed to bison specific mutagens. Usually covered in long thick fur.</description>
		<race>Bison</race>
		<classification>Bovid</classification>
		<categories>
			<li>Temperate</li>
			<li>Arid</li>
			<li>Bovid</li>
		</categories>
		<fullTransformation>PawnmorphBisonTF</fullTransformation>
		<partialTransformation>PawnmorphBisonTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(120,90,65)</skinColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>BisonTransformationMemory</transformationMemory>
			<revertedMemory>BisonRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>