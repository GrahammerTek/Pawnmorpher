<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef Name ="QuilledJawPart" ParentName="PawFootedJawPart" Abstract="true">
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>teeth</label>
						<labelUsedInLogging>False</labelUsedInLogging>
						<capacities>
							<li>Bite</li>
						</capacities>
						<power>2</power>
						<cooldownTime>1.5</cooldownTime>
						<alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
					</li>
				</tools>
			</li>
		</comps>
		<stagePatches>
			<li function="modify">
				<stageKey>initial</stageKey>
				<values>
					<verbOverrides>
						<li>
							<label>teeth</label>
							<power>0</power>
						</li>
					</verbOverrides>
				</values>
			</li>
			<li function="modify">
				<stageKey>pressing</stageKey>
				<values>
					<verbOverrides>
						<li>
							<label>teeth</label>
							<power>0</power>
						</li>
					</verbOverrides>
				</values>
			</li>
			<li function="modify">
				<stageKey>growing</stageKey>
				<values>
					<verbOverrides>
						<li>
							<label>teeth</label>
							<power>0</power>
						</li>
					</verbOverrides>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<verbOverrides>
						<li>
							<label>teeth</label>
							<power>1</power>
						</li>
					</verbOverrides>
					<statOffsets>
						<MedicalTendQualityOffset>0.025</MedicalTendQualityOffset>
					</statOffsets>
					<capMods>
						<li>
							<capacity>Talking</capacity>
							<offset>-0.2</offset>
						</li>
					</capMods>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapted</stageKey>
				<values>
					<statOffsets>
						<MedicalTendQualityOffset>0.05</MedicalTendQualityOffset>
					</statOffsets>
					<capMods>
						<li>
							<capacity>Talking</capacity>
							<offset>-0.1</offset>
						</li>
					</capMods>
				</values>
			</li>
			<li function="modify">
				<stageKey>paragon</stageKey>
				<values>
					<statOffsets>
						<MedicalTendQualityOffset>0.075</MedicalTendQualityOffset>
					</statOffsets>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef Name ="QuilledEarPart" ParentName="PawFootedEarPart" Abstract="true">
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef Name ="QuilledTailPart" ParentName="PawFootedTailPart" Abstract="true">
		<stagePatches>
			<li function="modify">
				<stageKey>growing</stageKey>
				<values>
					<statOffsets>
						<MedicalSurgerySuccessChance>+0.025</MedicalSurgerySuccessChance>
				</statOffsets>
				</values>
			</li>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<statOffsets>
						<MedicalSurgerySuccessChance>+0.05</MedicalSurgerySuccessChance>
					</statOffsets>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
</Defs>