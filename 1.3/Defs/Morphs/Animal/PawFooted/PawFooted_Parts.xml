<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef Name ="PawFootedJawPart" ParentName="AnimalJawPart" Abstract="true">
		<stagePatches>
			<li function="modify">
				<stageKey>pressing</stageKey>
				<values>
					<description>A snout graces [PAWN_nameDef]'s face, like that of a fox. [PAWN_possessive] teeth are pointed to suit; speaking is difficult, but eating is easier.</description>
					<graphics>
						<Muzzle>Parts/Partials/PartialMuzzle/TinySnout</Muzzle>
					</graphics>
				</values>
			</li>
			<li function="modify">
				<stageKey>growing</stageKey>
				<values>
					<description>A snout graces [PAWN_nameDef]'s face, like that of a fox. [PAWN_possessive] teeth are pointed to suit; speaking is difficult, but eating is easier. Adaptation has removed some of the difficulty with talking.</description>
					<graphics>
						<Muzzle>Parts/Partials/PartialMuzzle/PartialMuzzle</Muzzle>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef Name ="PawFootedEarPart" ParentName="AnimalEarPart" Abstract="true">
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef Name ="PawFootedTailPart" ParentName="AnimalTailPart" Abstract="true">
	</Pawnmorph.Hediffs.MutationDef>
</Defs>