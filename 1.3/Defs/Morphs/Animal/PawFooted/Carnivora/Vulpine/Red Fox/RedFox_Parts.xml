<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="FoxJawBase">
		<defName>EtherFoxMuzzle</defName>
		<classInfluence>FoxMorph</classInfluence>
		<label>red fox muzzle</label>
		<description>The vulpine muzzle of an red fox.</description>
		<graphics>
			<Muzzle>Parts/FoxUpdated/RedFox_Jaw</Muzzle>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/FoxUpdated/RedFox_Jaw</Muzzle>
					</graphics>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapted</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/FoxUpdated/RedFox_Jaw</Muzzle>
					</graphics>
				</values>
			</li>
			<li function="modify">
				<stageKey>paragon</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/FoxUpdated/RedFox_Jaw</Muzzle>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

