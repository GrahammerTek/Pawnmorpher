		<description>The vulpine ear of an arctic fox.</description>
		<graphics>
			<LeftEar>Parts/FoxArctic/ArcticFox_Ear_Left</LeftEar>
			<LeftEarBehind>Parts/FoxArctic/ArcticFox_Ear_Left_Behind</LeftEarBehind>
			<RightEar>Parts/FoxArctic/ArcticFox_Ear_Right</RightEar>
			<RightEarBehindHead>Parts/FoxArctic/ArcticFox_Ear_Right_Behind</RightEarBehindHead>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<graphics>
						<LeftEar>Parts/FoxArctic/ArcticFox_Ear_Left</LeftEar>
						<LeftEarBehind>Parts/FoxArctic/ArcticFox_Ear_Left_Behind</LeftEarBehind>
						<RightEar>Parts/FoxArctic/ArcticFox_Ear_Right</RightEar>
						<RightEarBehindHead>Parts/FoxArctic/ArcticFox_Ear_Right_Behind</RightEarBehindHead>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
