<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="FoxJawBase">
		<defName>EtherArcticFoxMuzzle</defName>
		<classInfluence>ArcticFoxMorph</classInfluence>
		<label>arctic fox muzzle</label>
		<description>The vulpine muzzle of an arctic fox.</description>
		<graphics>
			<Muzzle>Parts/FoxArctic/ArcticFox_Jaw</Muzzle>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/FoxArctic/ArcticFox_Jaw</Muzzle>
					</graphics>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapted</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/FoxArctic/ArcticFox_Jaw</Muzzle>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
