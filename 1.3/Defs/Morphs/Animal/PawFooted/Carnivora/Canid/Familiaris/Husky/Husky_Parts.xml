<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="FamiliarisJawPart">
		<classInfluence>HuskyMorph</classInfluence>
		<defName>EtherHuskyMuzzle</defName>
		<label>husky muzzle</label>
		<description>The wolf-like muzzle of a husky.</description>
		<graphics>
			<Muzzle>Parts/huskyface/huskyface</Muzzle>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/huskyface/huskyface</Muzzle>
					</graphics>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapted</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/huskyface/huskyface</Muzzle>
					</graphics>
				</values>
			</li>
			<li function="modify">
				<stageKey>paragon</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/huskyface/huskyface</Muzzle>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="FamiliarisEarPart">
		<classInfluence>HuskyMorph</classInfluence>
		<defName>EtherHuskyEar</defName>
		<label>husky ear</label>
		<description>Triangular, floofy black-and-white dog ears flick around at the sides of [PAWN_nameDef]'s head. They are incredibly cute.</description>
		<graphics>
			<LeftEar>Parts/Husky/Husky_Ear_Left</LeftEar>
			<LeftEarBehind>Parts/Husky/Husky_Ear_Left_Behind</LeftEarBehind>
			<RightEar>Parts/Husky/Husky_Ear_Right</RightEar>
			<RightEarBehindHead>Parts/Husky/Husky_Ear_Right_Behind</RightEarBehindHead>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<graphics>
						<LeftEar>Parts/Husky/Husky_Ear_Left</LeftEar>
						<LeftEarBehind>Parts/Husky/Husky_Ear_Left_Behind</LeftEarBehind>
						<RightEar>Parts/Husky/Husky_Ear_Right</RightEar>
						<RightEarBehindHead>Parts/Husky/Husky_Ear_Right_Behind</RightEarBehindHead>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="FamiliarisTailPart">
		<classInfluence>HuskyMorph</classInfluence>
		<defName>EtherHuskyTail</defName>
		<label>husky tail</label>
		<description>A wonderfully fluffy husky tail curls over [PAWN_nameDef]'s back. Was that a small wag just then?</description>
		<graphics>
			<Tail>Parts/huskyface/huskytail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<graphics>
						<Tail>Parts/huskyface/huskytail</Tail>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
</Defs>
