<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="FamiliarisJawPart">
		<classInfluence>TerrierMorph</classInfluence>
		<defName>EtherTerrierMuzzle</defName>
		<label>terrier muzzle</label>
		<description>The dog-like muzzle of a yorkshire terrier.</description>
		<graphics>
			<Muzzle>Parts/Terrier/Terrier_Jaw</Muzzle>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/Terrier/Terrier_Jaw</Muzzle>
					</graphics>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapted</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/Terrier/Terrier_Jaw</Muzzle>
					</graphics>
				</values>
			</li>
			<li function="modify">
				<stageKey>paragon</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/Terrier/Terrier_Jaw</Muzzle>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="FamiliarisEarPart">
		<classInfluence>TerrierMorph</classInfluence>
		<defName>EtherTerrierEar</defName>
		<label>terrier ear</label>
		<description>Fluffy, soft dog ears flick around at the sides of [PAWN_nameDef]'s head. They are incredibly cute.</description>
		<graphics>
			<LeftEar>Parts/Terrier/Terrier_Ear_Left</LeftEar>
			<LeftEarBehind>Parts/None/None</LeftEarBehind>
			<RightEar>Parts/Terrier/Terrier_Ear_Right</RightEar>
			<RightEarBehindHead>Parts/None/None</RightEarBehindHead>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<graphics>
						<LeftEar>Parts/Terrier/Terrier_Ear_Left</LeftEar>
						<LeftEarBehind>Parts/None/None</LeftEarBehind>
						<RightEar>Parts/Terrier/Terrier_Ear_Right</RightEar>
						<RightEarBehindHead>Parts/None/None</RightEarBehindHead>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="FamiliarisTailPart">
		<classInfluence>TerrierMorph</classInfluence>
		<defName>EtherTerrierTail</defName>
		<label>terrier tail</label>
		<description>A fluffy terrier tail points out from [PAWN_nameDef]'s back. Was that a small wag just then?</description>
		<graphics>
			<Tail>Parts/Terrier/Terrier_Tail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<graphics>
						<Tail>Parts/Terrier/Terrier_Tail</Tail>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
</Defs>
