<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="PantherineJawPart">
		<classInfluence>CougarMorph</classInfluence>
		<defName>EtherCougarJaw</defName>
		<label>cougar snout</label>
		<graphics>
			<Muzzle>Parts/Cougar/Cougar_Jaw</Muzzle>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<description>[PAWN_nameDef]'s face has pressed out into a cougar shape, which is useful for eating, though they may hiss and meow more than talk.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapted</stageKey>
				<values>
					<description>[PAWN_nameDef]'s face has pressed out into a cougar shape, which is useful for eating, though they may hiss and meow more than talk. Adaptation has removed some of the difficulty with speaking.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>paragon</stageKey>
				<values>
					<description>[PAWN_nameDef]'s face has pressed out into a cougar shape, which is useful for eating.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="PantherineEarPart">
		<classInfluence>CougarMorph</classInfluence>
		<defName>EtherCougarEar</defName>
		<label>cougar ear</label>
		<graphics>
			<LeftEar>Parts/Cougar/Cougar_Ear_Left</LeftEar>
			<LeftEarBehind>Parts/Cougar/Cougar_Ear_Left_Behind</LeftEarBehind>
			<RightEar>Parts/Cougar/Cougar_Ear_Right</RightEar>
			<RightEarBehindHead>Parts/Cougar/Cougar_Ear_Right_Behind</RightEarBehindHead>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>This ear has grown pointed and furred, keen and twitching, turning to listen to distant sounds.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="PantherineTailPart">
		<classInfluence>CougarMorph</classInfluence>
		<defName>EtherCougarTail</defName>
		<label>cougar tail</label>
		<graphics>
			<Tail>Parts/Cougar/Cougar_Tail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A long, sleek tail. It flicks back and forth whenever [PAWN_nameDef] is displeased with something.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
</Defs>
