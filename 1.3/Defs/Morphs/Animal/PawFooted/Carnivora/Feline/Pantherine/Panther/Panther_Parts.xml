<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="PantherineJawPart">
		<classInfluence>PantherMorph</classInfluence>
		<defName>EtherPantherJaw</defName>
		<label>pantherine muzzle</label>
		<graphics>
			<Muzzle>Parts/Panther/Panther_Jaw</Muzzle>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<description>[PAWN_nameDef]'s face has pressed out into a panther shape, which is useful for eating, though they may hiss and meow more than talk.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapted</stageKey>
				<values>
					<description>[PAWN_nameDef]'s face has pressed out into a panther shape, which is useful for eating, though they may hiss and meow more than talk. Adaptation has removed some of the difficulty with speaking.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>paragon</stageKey>
				<values>
					<description>[PAWN_nameDef]'s face has pressed out into a panther shape, which is useful for eating.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="PantherineEarPart">
		<classInfluence>PantherMorph</classInfluence>
		<defName>EtherPantherEar</defName>
		<label>panther ear</label>
		<graphics>
			<LeftEar>Parts/Panther/Panther_Ear_Left</LeftEar>
			<LeftEarBehind>Parts/Panther/Panther_Ear_Left_Behind</LeftEarBehind>
			<RightEar>Parts/Panther/Panther_Ear_Right</RightEar>
			<RightEarBehindHead>Parts/Panther/Panther_Ear_Right_Behind</RightEarBehindHead>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>Triangular fuzzy ears grace [PAWN_nameDef]'s head. They coil back when [PAWN_pronoun] is getting ready to pounce.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="PantherineTailPart">
		<classInfluence>PantherMorph</classInfluence>
		<defName>EtherPantherTail</defName>
		<label>pantherine tail</label>
		<graphics>
			<Tail>Parts/Panther/Panther_Tail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A long, sleek tail. It flicks back and forth whenever [PAWN_nameDef] is displeased with something.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
</Defs>