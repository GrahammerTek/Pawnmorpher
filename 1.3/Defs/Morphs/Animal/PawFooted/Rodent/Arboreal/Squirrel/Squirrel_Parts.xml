<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="ArborealJawPart">
		<classInfluence>SquirrelMorph</classInfluence>
		<defName>EtherSquirrelJaw</defName>
		<label>sciurine muzzle</label>
		<graphics>
			<Muzzle>Parts/Squirrel/Squirrel_Jaw</Muzzle>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<description>A snout graces [PAWN_nameDef]'s face, like that of a squirrel, complete with oversized front teeth; speaking is difficult, but eating is easier.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapted</stageKey>
				<values>
					<description>A snout graces [PAWN_nameDef]'s face, like that of a squirrel, complete with oversized front teeth; speaking is difficult, but eating is easier. Adaptation has removed some of the difficulty with talking.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>paragon</stageKey>
				<values>
					<description>An imposing snout graces [PAWN_nameDef]'s face, like that of a squirrel, complete with oversized front teeth.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="ArborealEarPart">
		<classInfluence>SquirrelMorph</classInfluence>
		<defName>EtherSquirrelEar</defName>
		<label>squirrel ear</label>
		<graphics>
			<LeftEar>Parts/Squirrel/Squirrel_Ear_Left</LeftEar>
			<LeftEarBehind>Parts/Squirrel/Squirrel_Ear_Left_Behind</LeftEarBehind>
			<RightEar>Parts/Squirrel/Squirrel_Ear_Right</RightEar>
			<RightEarBehindHead>Parts/Squirrel/Squirrel_Ear_Right_Behind</RightEarBehindHead>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>[PAWN_nameDef]'s ears have grown into small wedges, flicking about to listen for the beating of wings.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="ArborealTailPart">
		<classInfluence>SquirrelMorph</classInfluence>
		<defName>EtherSquirrelTail</defName>
		<label>sciurine tail</label>
		<graphics>
			<Tail>Parts/Squirrel/Squirrel_Tail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A long fluffy tail curls up from [PAWN_nameDef]'s waist. [PAWN_pronoun] loves to curl it up around [PAWN_objective]self and use it as a pillow while sleeping.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
</Defs>
