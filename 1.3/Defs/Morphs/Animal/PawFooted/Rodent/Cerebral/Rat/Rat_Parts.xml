<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="CerebralJawPart">
		<classInfluence>RatMorph</classInfluence>
		<defName>EtherRatSnout</defName>
		<label>rodent snout</label>
		<description>The ragged and ratty snout of a rambunctious rodent.</description>
		<graphics>
			<Muzzle>Parts/ratface/ratface</Muzzle>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/ratface/ratface</Muzzle>
					</graphics>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapted</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/ratface/ratface</Muzzle>
					</graphics>
				</values>
			</li>
			<li function="modify">
				<stageKey>paragon</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/ratface/ratface</Muzzle>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="CerebralEarPart">
		<classInfluence>RatMorph</classInfluence>
		<defName>EtherRatEar</defName>
		<label>rat ear</label>
		<description>The rounds ears of a rat.</description>
		<graphics>
			<LeftEar>Parts/Rat/Rat_Ear_Left</LeftEar>
			<LeftEarBehind>Parts/Rat/Rat_Ear_Left_Behind</LeftEarBehind>
			<RightEar>Parts/Rat/Rat_Ear_Right</RightEar>
			<RightEarBehindHead>Parts/Rat/Rat_Ear_Right_Behind</RightEarBehindHead>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>Grey-furred, rounded ears listen carefully for predators.</description>
					<graphics>
						<LeftEar>Parts/Rat/Rat_Ear_Left</LeftEar>
						<LeftEarBehind>Parts/Rat/Rat_Ear_Left_Behind</LeftEarBehind>
						<RightEar>Parts/Rat/Rat_Ear_Right</RightEar>
						<RightEarBehindHead>Parts/Rat/Rat_Ear_Right_Behind</RightEarBehindHead>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="CerebralTailPart">
		<classInfluence>RatMorph</classInfluence>
		<defName>EtherRatTail</defName>
		<label>hairless tail</label>
		<description>The long tail of a rat.</description>
		<graphics>
			<Tail>Parts/ratface/rattail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A ratty tail, hairless, long and snaking.</description>
					<graphics>
						<Tail>Parts/ratface/rattail</Tail>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="PawnmorphPart">
		<defName>EtherDextrousPawHand</defName>
		<label>dextrous paw</label>
		<description>Thankfully, this more manipulative paw allows [PAWN_nameDef] to keep their thumbs.</description>
		<initialSeverity>0</initialSeverity>
		<classInfluence>Rodent</classInfluence>
		<parts>
			<li>Hand</li>
		</parts>
		<stages>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<partEfficiencyOffset>-0.15</partEfficiencyOffset>
				<label>unfamiliar</label>
			</li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<minSeverity>0.99</minSeverity>
				<label>adapted</label>
			</li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<minSeverity>1.5</minSeverity> <!-- Adaption Cream -->
				<label>enhanced</label>
				<partEfficiencyOffset>0.1</partEfficiencyOffset>
			</li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<minSeverity>2.5</minSeverity> <!-- Paragon -->
				<label>paragon</label>
				<partEfficiencyOffset>0.2</partEfficiencyOffset>
			</li>
		</stages>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>claws</label>
						<labelUsedInLogging>False</labelUsedInLogging>
						<capacities>
							<li>Scratch</li>
						</capacities>
						<power>2.0</power>
						<cooldownTime>1</cooldownTime>
						<alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
					</li>
				</tools>
			</li>
			<li Class="Pawnmorph.Hediffs.CompProperties_MutationSeverityAdjust">
				<severityPerDay>0.25</severityPerDay>
			</li>
		</comps>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="PawnmorphPart">
		<defName>EtherRatFoot</defName>
		<label>rodent</label>
		<description>[PAWN_nameDef]'s feet are dextrous and elongated, but hairless and pale.</description>
		<parts>
			<li>Foot</li>
		</parts>
		<stages>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<capMods>
					<li>
						<capacity>Moving</capacity>
						<offset>0.05</offset>
					</li>
				</capMods>
			</li>
		</stages>
	</Pawnmorph.Hediffs.MutationDef>
</Defs>
