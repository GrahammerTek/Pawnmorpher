<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="CerebralJawPart">
		<classInfluence>ChinchillaMorph</classInfluence>
		<defName>EtherChinchillaSnout</defName>
		<label>chinchilla snout</label>
		<description>A cute chinchilla snout</description>
		<graphics>
			<Muzzle>Parts/Chinchilla/Chinchilla_Jaw</Muzzle>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/Chinchilla/Chinchilla_Jaw</Muzzle>
					</graphics>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapted</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/Chinchilla/Chinchilla_Jaw</Muzzle>
					</graphics>
				</values>
			</li>
			<li function="modify">
				<stageKey>paragon</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/Chinchilla/Chinchilla_Jaw</Muzzle>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="CerebralEarPart">
		<classInfluence>ChinchillaMorph</classInfluence>
		<defName>EtherChinchillaEar</defName>
		<label>chinchilla ear</label>
		<description>The large and smooth ears of a chinchilla.</description>
		<graphics>
			<LeftEar>Parts/Chinchilla/Chinchilla_Ear_Left</LeftEar>
			<LeftEarBehind>Parts/Chinchilla/Chinchilla_Ear_Left_Behind</LeftEarBehind>
			<RightEar>>Parts/Chinchilla/Chinchilla_Ear_Right</RightEar>
			<RightEarBehindHead>Parts/Chinchilla/Chinchilla_Ear_Right_Behind</RightEarBehindHead>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>Large ears clad in silky smooth fur has grown from [PAWN_nameDef]'s head, giving them an irresistably cute appearance.</description>
					<graphics>
						<LeftEar>Parts/Chinchilla/Chinchilla_Ear_Left</LeftEar>
						<LeftEarBehind>Parts/Chinchilla/Chinchilla_Ear_Left_Behind</LeftEarBehind>
						<RightEar>>Parts/Chinchilla/Chinchilla_Ear_Right</RightEar>
						<RightEarBehindHead>Parts/Chinchilla/Chinchilla_Ear_Right_Behind</RightEarBehindHead>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="CerebralTailPart">
		<classInfluence>ChinchillaMorph</classInfluence>
		<defName>EtherChinchillaTail</defName>
		<label>chinchilla tail</label>
		<description>The smooth and fluffy tail of a chinchilla.</description>
		<graphics>
			<Tail>Parts/Chinchilla/Chinchilla_Tail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A soft, fluffy tail with luxuriously smooth fur has grown from the base of [PAWN_nameDef]'s spine, charming and tempting everyone to touch it.</description>
					<graphics>
						<Tail>Parts/Chinchilla/Chinchilla_Tail</Tail>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
</Defs>
