<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef Name ="RodentJawPart" ParentName="PawFootedJawPart" Abstract="true">
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>teeth</label>
						<labelUsedInLogging>False</labelUsedInLogging>
						<capacities>
							<li>Bite</li>
						</capacities>
						<power>10.0</power>
						<cooldownTime>1.5</cooldownTime>
						<alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
					</li>
				</tools>
			</li>
		</comps>
		<stagePatches>
			<li function="initial">
				<stageKey>pressing</stageKey>
				<values>
					<verbOverrides>
						<li>
							<label>teeth</label>
							<power>0</power>
						</li>
					</verbOverrides>
				</values>
			</li>
			<li function="modify">
				<stageKey>pressing</stageKey>
				<values>
					<verbOverrides>
						<li>
							<label>teeth</label>
							<power>0</power>
						</li>
					</verbOverrides>
				</values>
			</li>
			<li function="modify">
				<stageKey>growing</stageKey>
				<values>
					<description>It's obvious now that a rather rodent-like snout is making its way onto [PAWN_nameDef]'s face. Whiskers, long front teeth, and a pointed nose are still taking shape.</description>
					<verbOverrides>
						<li>
							<label>teeth</label>
							<power>0</power>
						</li>
					</verbOverrides>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<verbOverrides>
						<li>
							<label>teeth</label>
							<power>5</power>
						</li>
					</verbOverrides>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef Name ="RodentEarPart" ParentName="PawFootedEarPart" Abstract="true">
		<stagePatches>
			<li function="modify">
				<stageKey>growing</stageKey>
				<values>
					<capMods>
						<li>
							<capacity>Hearing</capacity>
							<offset>+0.05</offset>
						</li>
					</capMods>
				</values>
			</li>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<capMods>
						<li>
							<capacity>Hearing</capacity>
							<offset>+0.1</offset>
						</li>
					</capMods>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef Name ="RodentTailPart" ParentName="PawFootedTailPart" Abstract="true">
	</Pawnmorph.Hediffs.MutationDef>
</Defs>