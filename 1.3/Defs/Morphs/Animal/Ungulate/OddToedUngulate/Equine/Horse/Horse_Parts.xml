<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="EquineJawPart">
		<classInfluence>HorseMorph</classInfluence>
		<defName>EtherHorseSnout</defName>
		<label>horse snout</label>
		<graphics>
			<Muzzle>Parts/horseface/horseface</Muzzle>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<description>A snout graces [PAWN_nameDef]'s face, like that of a horse. [PAWN_possessive] teeth are squared off; speaking is more difficult, but eating is easier.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapted</stageKey>
				<values>
					<description>A snout graces [PAWN_nameDef]'s face, like that of a horse. [PAWN_possessive] teeth are squared off; speaking is more difficult, but eating is easier. Adaptation has removed some of the difficulty with speaking.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="EquineEarPart">
		<classInfluence>HorseMorph</classInfluence>
		<defName>EtherHorseEar</defName>
		<label>horse ear</label>
		<graphics>
			<LeftEar>Parts/Horse/Horse_Ear_Left</LeftEar>
			<LeftEarBehind>Parts/Horse/Horse_Ear_Left_Behind</LeftEarBehind>
			<RightEar>Parts/Horse/Horse_Ear_Right</RightEar>
			<RightEarBehindHead>Parts/Horse/Horse_Ear_Right_Behind</RightEarBehindHead>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A long, equine ear sits atop [PAWN_nameDef]'s head, like that of a horse.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="EquineTailPart">
		<defName>EtherHorseTail</defName>
		<label>horse tail</label>
		<graphics>
			<Tail>Parts/horseface/horsetail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A horselike tail. It swats at flies occasionally.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
</Defs>
