<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="DomesticatedBovidJawPart">
		<classInfluence>YakMorph</classInfluence>
		<defName>EtherYakSnout</defName>
		<label>yak snout</label>
		<graphics>
			<Muzzle>Parts/Yak/Yak_Jaw</Muzzle>
			<BirdJawOrnament>Parts/Yak/Yak_Headfluff</BirdJawOrnament>
		</graphics>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="DomesticatedBovidEarPart">
		<classInfluence>YakMorph</classInfluence>
		<defName>EtherYakEar</defName>
		<label>yak ear</label>
		<graphics>
			<LeftEar>Parts/Yak/Yak_Ear_Left</LeftEar>
			<LeftEarBehind>Parts/Yak/Yak_Ear_Left_Behind</LeftEarBehind>
			<RightEar>Parts/Yak/Yak_Ear_Right</RightEar>
			<RightEarBehindHead>Parts/Yak/Yak_Ear_Right_Behind</RightEarBehindHead>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A large wooly ears that belongs to a yak. It flicks idly.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="DomesticatedBovidTailPart">
		<classInfluence>YakMorph</classInfluence>
		<defName>EtherYakTail</defName>
		<label>yak tail</label>
		<graphics>
			<Tail>Parts/Yak/Yak_Tail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A fluffy yak tail.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
</Defs>
