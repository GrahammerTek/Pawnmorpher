<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="BovidJawPart">
		<classInfluence>YakMorph</classInfluence>
		<defName>EtherYakSnout</defName>
		<label>yak snout</label>
		<graphics>
			<Muzzle>Parts/Yak/Yak_Jaw</Muzzle>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<description>A yaklike snout graces [PAWN_nameDef]'s face.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapted</stageKey>
				<values>
					<description>A yaklike snout graces [PAWN_nameDef]'s face. Adaptation has removed some of the difficulty with talking.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>paragon</stageKey>
				<values>
					<description>A yaklike snout graces [PAWN_nameDef]'s face. Further adaptation to this snout has allowed [PAWN_nameDef] to speak normally.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="BovidEarPart">
		<classInfluence>YakMorph</classInfluence>
		<defName>EtherYakEar</defName>
		<label>yak ear</label>
		<graphics>
			<LeftEar>Parts/Yak/Yak_Ear_Left</LeftEar>
			<LeftEarBehind>Parts/Yak/Yak_Ear_Left_Behind</LeftEarBehind>
			<RightEar>Parts/Yak/Yak_Ear_Right</RightEar>
			<RightEarBehindHead>Parts/Yak/Yak_Ear_Right_Behind</RightEarBehindHead>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A large wooly ears that belongs to a yak. It flicks idly.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="BovidTailPart">
		<classInfluence>YakMorph</classInfluence>
		<defName>EtherYakTail</defName>
		<label>yak tail</label>
		<graphics>
			<Tail>Parts/Yak/Yak_Tail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A fluffy yak tail.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

	<Pawnmorph.Hediffs.MutationDef ParentName="WoollyLimb">
		<defName>EtherYakFur</defName>
		<classInfluence>YakMorph</classInfluence>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="YakPart">
		<defName>EtherYakHorns</defName>
		<label>horns</label>
		<description>Two slender horns grow from the sides of [PAWN_nameDef]'s head.</description>
		<stages>
			<li Class="Pawnmorph.Hediffs.MutationStage">
			</li>
		</stages>
		<parts>
			<li>Skull</li>
		</parts>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>gored</label>
						<labelUsedInLogging>False</labelUsedInLogging>
						<capacities>
							<li>Scratch</li>
						</capacities>
						<power>20.0</power>
						<cooldownTime>3</cooldownTime>
						<alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
					</li>
				</tools>
			</li>
		</comps>
	</Pawnmorph.Hediffs.MutationDef>
</Defs>
