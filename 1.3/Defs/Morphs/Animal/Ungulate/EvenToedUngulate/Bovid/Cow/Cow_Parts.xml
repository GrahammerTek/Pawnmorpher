<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="BovidJawPart">
		<classInfluence>CowMorph</classInfluence>
		<defName>EtherCowSnout</defName>
		<label>cow snout</label>
		<graphics>
			<Muzzle>Parts/cowface/cowface</Muzzle>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>adapting</stageKey>
				<values>
					<description>A cowlike snout graces [PAWN_nameDef]'s face. The strange shape makes speaking very difficult.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>adapted</stageKey>
				<values>
					<description>A cowlike snout graces [PAWN_nameDef]'s face. Adapting to it has made speaking less frustrating.</description>
				</values>
			</li>
			<li function="modify">
				<stageKey>paragon</stageKey>
				<values>
					<description>A cowlike snout graces [PAWN_nameDef]'s face. With further adaptation, [PAWN_nameDef] has learned to speak normally.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="BovidEarPart">
		<classInfluence>CowMorph</classInfluence>
		<defName>EtherCowEar</defName>
		<label>cow ear</label>
		<graphics>
			<LeftEar>Parts/Cow/Cow_Ear_Left</LeftEar>
			<LeftEarBehind>Parts/Cow/Cow_Ear_Left_Behind</LeftEarBehind>
			<RightEar>Parts/Cow/Cow_Ear_Right</RightEar>
			<RightEarBehindHead>Parts/Cow/Cow_Ear_Right_Behind</RightEarBehindHead>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A large tan ear that belongs to a cow. It flicks idly. Maybe someone should tag it.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="BovidTailPart">
		<classInfluence>CowMorph</classInfluence>
		<defName>EtherCowTail</defName>
		<label>cow tail</label>
		<graphics>
			<Tail>Parts/cowface/cowtail</Tail>
		</graphics>
		<stagePatches>
			<li function="modify">
				<stageKey>grown</stageKey>
				<values>
					<description>A cowlike tail. It swats at flies and hangs low at their legs.</description>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>
	
	<Pawnmorph.Hediffs.MutationDef ParentName="PawnmorphPart">
		<defName>EtherHorns</defName>
		<classInfluence>CowMorph</classInfluence>
		<label>horns</label>
		<description>Burly horns decorate [PAWN_nameDef]'s head.</description>
		<stages>
			<li Class="Pawnmorph.Hediffs.MutationStage">
			</li>
		</stages>
		<parts>
			<li>Skull</li>
		</parts>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>gored</label>
						<labelUsedInLogging>False</labelUsedInLogging>
						<capacities>
							<li>Scratch</li>
						</capacities>
						<power>20.0</power>
						<cooldownTime>3</cooldownTime>
						<alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
					</li>
				</tools>
			</li>
		</comps>
	</Pawnmorph.Hediffs.MutationDef>
</Defs>
