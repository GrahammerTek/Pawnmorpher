<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef Name ="ReptileJawPart" ParentName="AnimalJawPart" Abstract="true">
		<stagePatches>
			<li function="modify">
				<stageKey>pressing</stageKey>
				<values>
					<graphics>
						<Muzzle>Parts/Partials/PartialMuzzle/TinySnout</Muzzle>
					</graphics>
				</values>
			</li>
			<li function="modify">
				<stageKey>growing</stageKey>
				<values>
					<description>[PAWN_nameDef]'s jaw and chin continue to press outwards. Sections of [PAWN_possesive] skin are peeling off to reveal freshly formed scales.</description>
					<graphics>
						<Muzzle>Parts/Partials/PartialMuzzle/PartialMuzzle</Muzzle>
					</graphics>
				</values>
			</li>
		</stagePatches>
	</Pawnmorph.Hediffs.MutationDef>

