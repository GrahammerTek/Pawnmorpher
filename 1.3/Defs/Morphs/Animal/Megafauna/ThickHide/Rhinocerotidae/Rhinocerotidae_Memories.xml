<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../Schemas/ThoughtDef.xsd">

	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherRhinocerotidaeJaw</defName>
		<stages>
			<li>
				<label>Rhino horn</label>
				<description>Let's hope the colony doesn't want to mount my head on a wall...</description>
				<visible>true</visible>
				<baseMoodEffect>-1</baseMoodEffect>
			</li>
			<li>
				<label>Rhino horn</label>
				<description>Look at this majestic horn, I feel powerful!</description>
				<visible>true</visible>
				<baseMoodEffect>2</baseMoodEffect>
			</li>
			<li>
				<label>Rhino horn</label>
				<description>I WILL STAB SOMEBODY WITH THIS THING.</description>
				<visible>true</visible>
				<baseMoodEffect>-3</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>
	
	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherRhinocerotidaeEar</defName>
		<stages>
			<li>
				<label>Rhino ears</label>
				<description>Did someone stick some tubes on my ears?</description>
				<visible>true</visible>
				<baseMoodEffect>-1</baseMoodEffect>
			</li>
			<li>
				<label>Rhino ears</label>
				<description>Perfect for healing and cooling you off a bit!</description>
				<visible>true</visible>
				<baseMoodEffect>1</baseMoodEffect>
			</li>
			<li>
				<label>Rhino ears</label>
				<description>I'm used to my ears, not these weird tubes...</description>
				<visible>true</visible>
				<baseMoodEffect>-2</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>
	
	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherRhinocerotidaeTail</defName>
		<stages>
			<li>
				<label>Rhino tail</label>
				<description>It's just a useless piece of skin hanging of me...</description>
				<visible>true</visible>
				<baseMoodEffect>-1</baseMoodEffect>
			</li>
			<li>
				<label>Rhino tail</label>
				<description>Well...it's a tail, I guess.</description>
				<visible>true</visible>
				<baseMoodEffect>1</baseMoodEffect>
			</li>
			<li>
				<label>Rhino tail</label>
				<description>Why do I even have this...weird tail thing?!</description>
				<visible>true</visible>
				<baseMoodEffect>-2</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>
	
</Defs>
