<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef ParentName="HerdMorph">
		<defName>MuffaloMorph</defName>
		<label>muffalomorph</label>
		<description>A large humanoid resulting when a human is exposed to muffalo specific mutagens. Usually covered in long thick fur.</description>
		<race>Muffalo</race>
		<classification>Bovid</classification>
		<categories>
			<li>Temperate</li>
			<li>Cold</li>
			<li>Production</li>
			<li>Bovid</li>
		</categories>
		<associatedAnimals>
			<li>Muffalo</li>
		</associatedAnimals>
		<fullTransformation>PawnmorphMuffaloTF</fullTransformation>
		<partialTransformation>PawnmorphMuffaloTFPartial</partialTransformation>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(180,196,200)</skinColorOverride>
			</graphicsSettings>
		</raceSettings>
		<transformSettings>
			<transformationMemory>MuffaloTransformationMemory</transformationMemory>
			<revertedMemory>MuffaloRevertedMemory</revertedMemory>
		</transformSettings>
	</Pawnmorph.MorphDef>
</Defs>