	<!--This one is the definitive one, remove Tmp when hierarchy done-->
	<Pawnmorph.Hediffs.MutationDef Name="PawnmorphSkinPartTmp" Abstract="true" ParentName="PawnmorphPartNoRmComp">
		<description>A covered limb.</description>
		<initialSeverity>0</initialSeverity>
		<maxSeverity>1</maxSeverity>
		<parts>
			<li>Torso</li>
			<li>Head</li>
			<li>Ear</li>
			<li>Arm</li>
			<li>Hand</li>
			<li>Leg</li>
			<li>Foot</li>
			<li>Tail</li>
			<li>MorphTail</li>
		</parts>
		<comps>
			<li Class="Pawnmorph.Hediffs.CompProperties_MutationSeverityAdjust">
				<severityPerDay>0.33</severityPerDay>
			</li>
			<li Class="Pawnmorph.Hediffs.RemoveFromPartCompProperties">
				<layer>Skin</layer>
			</li>
			<li Class="Pawnmorph.Hediffs.SpreadingMutationCompProperties">
				<mtb>2</mtb>
				<maxTreeSearchDepth>1</maxTreeSearchDepth>
			</li>
		</comps>
		<stages>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<key>light</key>
				<label>light</label>
				<minSeverity>0</minSeverity>
			</li>
			<li Class="Pawnmorph.Hediffs.MutationStage">
				<key>dusting</key>
				<label>dusting</label>
				<minSeverity>0</minSeverity>
