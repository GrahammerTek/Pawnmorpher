<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MutationDef.xsd">
	<Pawnmorph.Hediffs.MutationDef ParentName="PawnmorphHornPart">
		<classInfluence>ThrumboMorph</classInfluence>
		<defName>EtherThrumboHorn</defName>
		<label>thrumbohorn</label>
		<!-- <description>A razor-sharp thrumbo horn decorates [PAWN_nameDef]'s head.</description> -->
        <description>A razor-sharp thrumbo horn.</description>
		<mutationTale>GrewThrumboHorn</mutationTale>
		<spawnThingOnRemoved>ThrumboHorn</spawnThingOnRemoved>
		<value>30</value>
		<categories>
			<li>Powerful</li>
		</categories>
		<stagePatches>
			<li function="modify">
				<stageKey>initial</stageKey>
				<values>
                    <description>TODO</description>
					<verbOverrides>
                        <li>
                            <label>point</label>
                            <power>0</power>
                        </li>
                        <li>
                            <label>base</label>
                            <power>0</power>
                        </li>
                    </verbOverrides>
				</values>
			</li>
			<li function="modify">
				<stageKey>nubs</stageKey>
				<values>
                    <label>nub</label>
                    <description>[PAWN_nameDef] has grown a small hard nub on [PAWN_possessive] forhead. Maybe good for a headbutt but not much else.</description>
					<verbOverrides>
                        <li>
                            <label>point</label>
                            <power>0</power>
                        </li>
                        <li>
                            <label>base</label>
                            <power>3</power>
                        </li>
                    </verbOverrides>
				</values>
			</li>
			<li function="modify">
				<stageKey>short</stageKey>
				<values>
                    <description>[PAWN_nameDef] has grown what looks like a small version of a thrumbo's horn. Not quite as sharp or powerful as a fully grown thrumbo horn, but can still inflict some damage.</description>
                    <statOffsets>
                        <MeleeArmorPenetration>0.1</MeleeArmorPenetration>
                    </statOffsets>
					<verbOverrides>
                        <li>
                            <label>point</label>
                            <power>14</power>
                        </li>
                        <li>
                            <label>base</label>
                            <power>6</power>
                        </li>
                    </verbOverrides>
				</values>
			</li>
			<li function="modify">
				<stageKey>horns</stageKey>
				<values>
                    <label></label>
                    <description>A large and majestic horn near identical to what you find on a thrumbo adorns [PAWN_nameDef]'s head. It is a very powerful and sharp horn.</description>
                    <statOffsets>
                        <MeleeArmorPenetration>0.3</MeleeArmorPenetration>
                    </statOffsets>
					<verbOverrides>
                        <li>
                            <label>point</label>
                            <power>28</power>
                        </li>
                        <li>
                            <label>base</label>
                            <power>9</power>
                        </li>
                    </verbOverrides>
				</values>
			</li>
		</stagePatches>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>point</label>
						<capacities>
							<li>Scratch</li>
							<li>Stab</li>
						</capacities>
						<power>0</power>
						<cooldownTime>2.6</cooldownTime>
					</li>
					<li>
						<label>base</label>
						<capacities>
							<li>Blunt</li>
						</capacities>
						<power>0</power>
						<cooldownTime>2</cooldownTime>
					</li>
				</tools>
			</li>
		</comps>
	</Pawnmorph.Hediffs.MutationDef>
    
	<Pawnmorph.Hediffs.MutationDef ParentName="PawnmorphPart">
		<classInfluence>PM_HellhoundMorph</classInfluence>
		<defName>PM_HellhoundHorns</defName>
		<label>hellhound horns</label>
		<description>[PAWN_nameDef] has grown a pair of large horns from their skull</description>
		<graphics>
			<Horns>Parts/COMBINATIONS/Hellhound/Hellhound_Horns</Horns>
		</graphics>
		<categories>
			<li>Major</li>
			<li>Combo</li>
		</categories>
		<parts>
			<li>Skull</li>
		</parts>
		<stages>
			<li Class="Pawnmorph.Hediffs.MutationStage"></li>
		</stages>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>hellhound horns</label>
						<labelUsedInLogging>False</labelUsedInLogging>
						<capacities>
							<li>Scratch</li>
						</capacities>
						<power>9.0</power>
						<cooldownTime>1.5</cooldownTime>
						<alwaysTreatAsWeapon>true</alwaysTreatAsWeapon>
					</li>
				</tools>
			</li>
		</comps>
	</Pawnmorph.Hediffs.MutationDef>

