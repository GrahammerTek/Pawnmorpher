<Defs>
	<TraderKindDef>
		<defName>PawnmorpherTrader</defName>
		<label>Morph trader</label>
		<orbital>false</orbital>
		<commonality>1</commonality>
		<stockGenerators>
			<!-- Ressources -->
			<li Class="StockGenerator_SingleDef">
				<thingDef>Silver</thingDef>
				<countRange>750~1200</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>ComponentIndustrial</thingDef>
				<countRange>6~20</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Steel</thingDef>
				<countRange>75~200</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Plasteel</thingDef>
				<countRange>50~150</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Mutanite</thingDef>
				<countRange>1~10</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Chemfuel</thingDef>
				<countRange>100~200</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Neutroamine</thingDef>
				<countRange>70~160</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>MedicineIndustrial</thingDef>
				<countRange>2~15</countRange>
			</li>

			<!-- Drugs -->
			<li Class="StockGenerator_SingleDef">
				<thingDef>MechaniteSlurry</thingDef>
				<countRange>6~10</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>Antigen</thingDef>
				<countRange>1~5</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>MutagenStabiliser</thingDef>
				<countRange>1~3</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>MutagenAccelerator</thingDef>
				<countRange>1~3</countRange>
			</li>
			<li Class="StockGenerator_MultiDef">
				<thingDefs>
					<li>UdderPill</li>
					<li>DryPills</li>
				</thingDefs>
				<countRange>-1~2</countRange><!-- 50% chance -->
			</li>
			<li Class="StockGenerator_MultiDef">
				<thingDefs>
					<li>FeralPill</li>
					<li>SapiencePill</li>
				</thingDefs>
				<countRange>-1~2</countRange><!-- 50% chance -->
			</li>
			<li Class="StockGenerator_MultiDef">
				<thingDefs>
					<li>ColorsPill</li>
					<li>AspectReverterPills</li><!-- 50% chance -->
				</thingDefs>
				<countRange>-1~2</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>ReversionSyringe</thingDef>
				<countRange>-2~1</countRange><!-- 25% chance -->
			</li>
			<li Class="StockGenerator_Category">
				<categoryDef>Serum</categoryDef>
				<excludedThingDefs>
					<li>RandomPowerfulTransformer</li>
				</excludedThingDefs>
				<thingDefCountRange>2~4</thingDefCountRange>
				<countRange>1~1</countRange>
			</li>
			<li Class="StockGenerator_SingleDef">
				<thingDef>RandomPowerfulTransformer</thingDef>
				<countRange>-18~1</countRange><!-- 5% chance -->
			</li>
			<li Class="Pawnmorph.StockGenerator_Injectors">
				<animalClass>Animal</animalClass><!-- this will grab injectors from all morphs under the given category -->
				<countRange>-7~2</countRange><!-- 10% chance for 1, 10% for 2-->
			</li>
			
			<!-- Techprints -->
			<!-- none -->

			<!-- Weapons -->
			<!-- none -->

			<!-- Apparel -->
			<!-- none -->

			<!-- Implants -->
			<!-- none -->

			<!-- Buildings -->
			<!-- none -->

			<!-- Exotic -->
			<li Class="Pawnmorph.StockGenerators.GenomeGenerator">
				<allowRestricted>false</allowRestricted>
				<countRange>-1~5</countRange>
			</li>

			<!-- Slaves -->
			<li Class="Pawnmorph.StockGenerator_MorphSlaves">
				<respectPopulationIntent>false</respectPopulationIntent>
				<countRange>2~10</countRange>
			</li>

			<!-- Animals -->
			<li Class="Pawnmorph.StockGenerator_Morphs">
				<checkTemperature>false</checkTemperature>
				<tradeTagsSell>
					<li>AnimalChaotic</li>
					<li>AnimalCommon</li>
					<li>AnimalUncommon</li>
					<li>AnimalFighter</li>
				</tradeTagsSell>
				<kindCountRange>1~5</kindCountRange>
				<countRange>1~5</countRange>
			</li>

			<!-- Buying -->
			<li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
			<li Class="StockGenerator_BuyExpensiveSimple" />
			<!-- weird organic gone and I don't know what replaced it 
			<li Class="StockGenerator_BuyWeirdOrganic" />
			-->
			
		</stockGenerators>
	</TraderKindDef>

