<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/ThoughtDef.xsd">
    <ThoughtDef ParentName="OutlookMemoryBase">
		<defName>ElkTransformationMemory</defName>
        <stages>
            <li>
                <label>!!!ElkTransformationMemory's default stage needs a label!!!</label>
                <description>!!!ElkTransformationMemory's default stage needs a description!!!</description>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
            <li>
                <label>!!!ElkTransformationMemory's mutation affinity stage needs a label!!!</label>
                <description>!!!ElkTransformationMemory's mutation affinity stage needs a description!!!</description>
                <baseMoodEffect>+3</baseMoodEffect>
            </li>
            <li>
                <label>!!!ElkTransformationMemory's body purist stage needs a label!!!</label>
                <description>!!!ElkTransformationMemory's body purist stage needs a description!!!</description>
                <baseMoodEffect>-5</baseMoodEffect>
            </li>
            <li>
                <label>!!!ElkTransformationMemory's primal wish stage needs a label!!!</label>
                <description>!!!ElkTransformationMemory's primal wish stage needs a description!!!</description>
                <baseMoodEffect>+1</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>
	
    <ThoughtDef ParentName="OutlookMemoryBase">
		<defName>ElkRevertedMemory</defName>
        <stages>
            <li>
                <label>!!!ElkRevertedMemory's default stage needs a label!!!</label>
                <description>!!!ElkRevertedMemory's default stage needs a description!!!</description>
                <baseMoodEffect>3</baseMoodEffect>
            </li>
            <li>
                <label>!!!ElkRevertedMemory's mutation affinity stage needs a label!!!</label>
                <description>!!!ElkRevertedMemory's mutation affinity stage needs a description!!!</description>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
            <li>
                <label>!!!ElkRevertedMemory's body purist stage needs a label!!!</label>
                <description>!!!ElkRevertedMemory's body purist stage needs a description!!!</description>
                <baseMoodEffect>10</baseMoodEffect>
            </li>
            <li>
                <label>!!!ElkRevertedMemory's primal wish stage needs a label!!!</label>
                <description>!!!ElkRevertedMemory's primal wish stage needs a description!!!</description>
                <baseMoodEffect>-5</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>

    <ThoughtDef ParentName="OutlookMemoryBase">
        <durationDays>1</durationDays>
        <defName>EtherElkJaw</defName>
        <stages>
            <li>
                <label>Elk face</label>
                <description>I can barely talk with this thing...</description>
                <visible>True</visible>
                <baseMoodEffect>-1</baseMoodEffect>
            </li>
            <li>
                <label>Elk face</label>
                <description>...This is kinda cool, I guess?</description>
                <visible>True</visible>
                <baseMoodEffect>2</baseMoodEffect>
            </li>
            <li>
                <label>Elk face</label>
                <description>What happened to my face?!?</description>
                <visible>True</visible>
                <baseMoodEffect>-3</baseMoodEffect>
            </li>
        </stages>
    </ThoughtDef>

	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherElkEar</defName>
		<stages>
			<li>
				<label>Elk ears</label>
				<description>At least they keep the flies off themselves.</description>
				<visible>true</visible>
				<baseMoodEffect>-1</baseMoodEffect>
			</li>
			<li>
				<label>Elk ears</label>
				<description>I can hear so well now!</description>
				<visible>true</visible>
				<baseMoodEffect>1</baseMoodEffect>
			</li>
			<li>
				<label>Elk ears</label>
				<description>These things are so twitchy, get them off me!</description>
				<visible>true</visible>
				<baseMoodEffect>-2</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>
	
	<ThoughtDef ParentName="OutlookMemoryBase">
		<durationDays>1</durationDays>
		<defName>EtherElkTail</defName>
		<stages>
			<li>
				<label>Elk tail</label>
				<description>Is this stubby little thing...a tail?</description>
				<visible>true</visible>
				<baseMoodEffect>-1</baseMoodEffect>
			</li>
			<li>
				<label>Elk tail</label>
				<description>It's so small and fluffy!</description>
				<visible>true</visible>
				<baseMoodEffect>1</baseMoodEffect>
			</li>
			<li>
				<label>Elk tail</label>
				<description>At least I can hide it.</description>
				<visible>true</visible>
				<baseMoodEffect>-2</baseMoodEffect>
			</li>
		</stages>
	</ThoughtDef>
	
</Defs>