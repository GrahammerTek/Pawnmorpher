<Defs>
    <HediffDef Abstract="true" Name="MorphDiseasBase" ParentName="InfectionBase">
        <hediffClass>Pawnmorph.Hediffs.MorphDisease</hediffClass>
        <makesAlert>true</makesAlert>
        <scenarioCanAdd>true</scenarioCanAdd>
    </HediffDef>

    <HediffDef ParentName="MorphDiseasBase">
        <defName>FoxFlu</defName>
        <label>fox flu</label>
        <makesSickThought>true</makesSickThought>
        <tendable>true</tendable>
        <comps>
            <li Class="HediffCompProperties_TendDuration">
                <baseTendDurationHours>12</baseTendDurationHours>
                <severityPerDayTended>-0.0773</severityPerDayTended>
            </li>
            <li Class="HediffCompProperties_Immunizable">
                <severityPerDayNotImmune>0.3088</severityPerDayNotImmune>
                <immunityPerDaySick>0.2708</immunityPerDaySick>
                <severityPerDayImmune>-0.2947</severityPerDayImmune>
                <immunityPerDayNotSick>-0.06</immunityPerDayNotSick>
            </li>
        </comps>
        <stages>
            <li>
                <label>minor</label>
                <capMods>
                    <li>
                        <capacity>Consciousness</capacity>
                        <offset>-0.05</offset>
                    </li>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>-0.05</offset>
                    </li>
                    <li>
                        <capacity>Breathing</capacity>
                        <offset>-0.1</offset>
                    </li>
                </capMods>
                <hediffGivers>
                    <li Class="Pawnmorph.HediffGiver_Mutation">
                        <hediff>EtherFurredLimb</hediff>
                        <mtbDays>2</mtbDays>
                        <partsToAffect>
                            <li>Arm</li>
                        </partsToAffect>
                        <countToAffect>2</countToAffect>
                        <tale>LimbGrowsFur</tale>
                    </li>
                    <li Class="Pawnmorph.HediffGiver_Mutation">
                            <hediff>EtherFoxEye</hediff>
                            <mtbDays>1.5</mtbDays>
                            <partsToAffect>
                                <li>Eye</li>
                            </partsToAffect>
                            <countToAffect>2</countToAffect>
                            <tale>EyesBecomeVulpine</tale>
                        </li>
                </hediffGivers>
            </li>
            <li>
                <minSeverity>0.666</minSeverity>
                <label>major</label>
                <vomitMtbDays>1.5</vomitMtbDays>
                <capMods>
                    <li>
                        <capacity>Consciousness</capacity>
                        <offset>-0.1</offset>
                    </li>
                    <li>
                        <capacity>Manipulation</capacity>
                        <offset>-0.1</offset>
                    </li>
                    <li>
                        <capacity>Breathing</capacity>
                        <offset>-0.15</offset>
