                <hediffGivers>
                    <li Class="Pawnmorph.HediffGiver_Mutation">
                        <hediff>EtherFurredLimb</hediff>
                        <mtbDays>1</mtbDays>
                        <partsToAffect>
                            <li>Arm</li>
                        </partsToAffect>
                        <countToAffect>2</countToAffect>
                        <tale>LimbGrowsFur</tale>
                    </li>
                    <li Class="Pawnmorph.HediffGiver_Mutation">
                        <hediff>EtherFoxEar</hediff>
                        <mtbDays>1</mtbDays>
                        <partsToAffect>
                            <li>Ear</li>
                        </partsToAffect>
                        <countToAffect>2</countToAffect>
