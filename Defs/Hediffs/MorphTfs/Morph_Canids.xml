<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <HediffDef ParentName="MorphParent">
    <defName>PawnmorphFoxTF</defName>
    <label>foxmorph</label>
    <stages>
      <li>
        <label>reeling</label>
        <minSeverity>0</minSeverity>
        <hungerRateFactor>1.6</hungerRateFactor>
        <painOffset>0.05</painOffset>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.05</offset>
          </li>
        </capMods>
        <hediffGivers>
          <li Class="Pawnmorph.HediffGiver_Esoteric_Chance">
            <hediff>EtherPawHand</hediff>
            <mtbDays>0.02</mtbDays>
            <partsToAffect>
              <li>Hand</li>
            </partsToAffect>
            <countToAffect>2</countToAffect>
          </li>
        </hediffGivers>
      </li>
      <li>
        <label>transforming</label>
        <minSeverity>0.64</minSeverity>
        <hungerRateFactor>1.6</hungerRateFactor>
        <painOffset>0.05</painOffset>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.05</offset>
          </li>
        </capMods>
        <hediffGivers>
          <li Class="Pawnmorph.HediffGiver_TF">
            <hediff>TransformedHuman</hediff>
            <pawnkinds>
              <li>Fox_Red</li>
            </pawnkinds>
            <tale>TransformationCanid</tale>
          </li>
        </hediffGivers>
      </li>
      <li>
        <label>changing</label>
        <minSeverity>0.65</minSeverity>
        <painOffset>0.1</painOffset>
        <vomitMtbDays>1</vomitMtbDays>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.05</offset>
          </li>
          <li>
            <capacity>Sight</capacity>
            <offset>-0.05</offset>
          </li>
          <li>
            <capacity>Manipulation</capacity>
            <offset>-0.05</offset>
          </li>
        </capMods>
        <hediffGivers>
          <li Class="Pawnmorph.HediffGiver_Esoteric_Fixed">
            <hediff>EtherFoxMuzzle</hediff>
            <mtbDays>2.5</mtbDays>
            <fixedParts>
              <li>Jaw</li>
            </fixedParts>
          </li>
          <li Class="Pawnmorph.HediffGiver_Esoteric">
            <hediff>EtherDigitigradeLeg</hediff>
            <mtbDays>2</mtbDays>
            <partsToAffect>
              <li>Leg</li>
            </partsToAffect>
            <countToAffect>2</countToAffect>
          </li>
          <li Class="Pawnmorph.HediffGiver_Esoteric">
            <hediff>EtherPawFoot</hediff>
            <mtbDays>2</mtbDays>
            <partsToAffect>
              <li>Foot</li>
            </partsToAffect>
            <countToAffect>2</countToAffect>
          </li>
          <li Class="Pawnmorph.HediffGiver_Esoteric">
            <hediff>EtherFurredLimb</hediff>
            <mtbDays>3</mtbDays>
            <partsToAffect>
              <li>Arm</li>
            </partsToAffect>
            <countToAffect>2</countToAffect>
          </li>
          <li Class="Pawnmorph.HediffGiver_Esoteric_Fixed">
            <hediff>EtherFluffyTail</hediff>
            <mtbDays>2</mtbDays>
            <fixedParts>
              <li>Tail</li>
            </fixedParts>
            <tale>GrewTail</tale>
          </li>
          <li Class="Pawnmorph.HediffGiver_Esoteric">
            <hediff>EtherFoxEye</hediff>
            <mtbDays>0.5</mtbDays>
            <partsToAffect>
              <li>Eye</li>
            </partsToAffect>
            <countToAffect>2</countToAffect>
          </li>
          <li Class="Pawnmorph.HediffGiver_Esoteric">
            <hediff>EtherFoxEar</hediff>
            <mtbDays>0.5</mtbDays>
            <partsToAffect>
              <li>Ear</li>
            </partsToAffect>
            <countToAffect>2</countToAffect>
          </li>
        </hediffGivers>
      </li>
    </stages>
  </HediffDef>

