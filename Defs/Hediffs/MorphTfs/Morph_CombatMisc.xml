      <li>
        <label>reeling</label>
        <minSeverity>0</minSeverity>
        <hungerRateFactor>1.6</hungerRateFactor>
        <painOffset>0.05</painOffset>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.05</offset>
          </li>
        </capMods>
        <hediffGivers>
          <li Class="Pawnmorph.HediffGiver_Esoteric_Chance">
            <hediff>EtherBearPawHand</hediff>
            <mtbDays>0.02</mtbDays>
            <partsToAffect>
              <li>Hand</li>
            </partsToAffect>
            <countToAffect>2</countToAffect>
          </li>
        </hediffGivers>
      </li>
      <li>
        <label>transforming</label>
        <minSeverity>0.64</minSeverity>
        <hungerRateFactor>1.6</hungerRateFactor>
        <painOffset>0.05</painOffset>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.05</offset>
          </li>
        </capMods>
        <hediffGivers>
          <li Class="Pawnmorph.HediffGiver_TF">
            <hediff>TransformedHuman</hediff>
            <pawnkinds>
              <li>Bear_Grizzly</li>
            </pawnkinds>
            <tale>TransformationMisc</tale>
          </li>
        </hediffGivers>
      </li>
      <li>
        <label>changing</label>
        <minSeverity>0.65</minSeverity>
        <painOffset>0.1</painOffset>
        <vomitMtbDays>1</vomitMtbDays>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.05</offset>
          </li>
          <li>
            <capacity>Sight</capacity>
            <offset>-0.05</offset>
          </li>
          <li>
            <capacity>Manipulation</capacity>
            <offset>-0.05</offset>
          </li>
        </capMods>
        <hediffGivers>
          <li Class="Pawnmorph.HediffGiver_Esoteric_Fixed">
            <hediff>EtherBearMuzzle</hediff>
            <mtbDays>3</mtbDays>
            <fixedParts>
              <li>Jaw</li>
            </fixedParts>
          </li>
          <li Class="Pawnmorph.HediffGiver_Esoteric">
            <hediff>EtherThickFurLimb</hediff>
            <mtbDays>2</mtbDays>
            <partsToAffect>
              <li>Leg</li>
            </partsToAffect>
            <countToAffect>2</countToAffect>
          </li>
          <li Class="Pawnmorph.HediffGiver_Esoteric">
            <hediff>EtherPawFoot</hediff>
            <mtbDays>2</mtbDays>
            <partsToAffect>
              <li>Foot</li>
            </partsToAffect>
            <countToAffect>2</countToAffect>
          </li>
          <li Class="Pawnmorph.HediffGiver_Esoteric">
            <hediff>EtherThickFurLimb</hediff>
            <mtbDays>3</mtbDays>
            <partsToAffect>
              <li>Arm</li>
            </partsToAffect>
            <countToAffect>2</countToAffect>
          </li>
          <li Class="Pawnmorph.HediffGiver_Esoteric_Fixed">
            <hediff>EtherBearTail</hediff>
            <mtbDays>2</mtbDays>
            <fixedParts>
              <li>Tail</li>
            </fixedParts>
            <tale>GrewTail</tale>
          </li>
          <li Class="Pawnmorph.HediffGiver_Esoteric">
            <hediff>EtherBearEar</hediff>
            <mtbDays>0.5</mtbDays>
            <partsToAffect>
              <li>Ear</li>
            </partsToAffect>
            <countToAffect>2</countToAffect>
          </li>
        </hediffGivers>
      </li>
