					<li>
						<capacity>Moving</capacity>
						<offset>-0.05</offset>
					</li>
				</capMods>
				<hediffGivers>
					<li Class="Pawnmorph.HediffGiver_TF">
						<hediff>EtherFoxEye</hediff>
						<pawnkind>Fox_Red</pawnkind>
						<tale>TransformationCanid</tale>
					</li>
					<li Class="Pawnmorph.HediffGiver_Esoteric_Chance">
						<hediff>EtherPawHand</hediff>
						<mtbDays>0.02</mtbDays>
						
						<partsToAffect>
							<li>Hand</li>
						</partsToAffect>
						<countToAffect>2</countToAffect>
					</li>
				</hediffGivers>
			</li>
			<li>
				<label>changing</label>
				
				<minSeverity>0.65</minSeverity>
				<painOffset>0.1</painOffset>
