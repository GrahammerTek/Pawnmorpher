<Defs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../Schemas/MorphDef.xsd">
	<Pawnmorph.MorphDef ParentName="HerdMorph">
		<defName>CowMorph</defName>
		<label>cowmorph</label>
		<description>A bovine humanoid resulting when a human is exposed to cow specific mutagens. Usually capable of producing milk.</description>
		<race>Cow</race>
		<categories>
			<li>Farm</li>
			<li>Production</li>
		</categories>
		<fullTransformation>PawnmorphCowTF</fullTransformation>
		<partialTransformation>PawnmorphCowTFPartial</partialTransformation>
		<classification>Bovidae</classification>
		<raceSettings>
			<graphicsSettings>
				<skinColorOverride>(255,255,255)</skinColorOverride>
			</graphicsSettings>
		</raceSettings>
	</Pawnmorph.MorphDef>
</Defs>