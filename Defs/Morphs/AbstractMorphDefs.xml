<Defs>
	<Pawnmorph.MorphDef Abstract="True" Name="SensitiveFoodMorph"> <!-- abstract def for all morphs that have bad thoughts when they eat meat of the same animal -->
		<raceSettings>
			<thoughtSettings>
				<ateAnimalThought>
					<thought>MorphAteAnimalMeatDirect</thought>
					<ingredientThought>MorphAteAnimalMeatAsIngredient</ingredientThought>
				</ateAnimalThought>
			</thoughtSettings>
		</raceSettings>
	</Pawnmorph.MorphDef>

  <Pawnmorph.MorphDef Abstract="True" Name="CanineMorph" ParentName="SensitiveFoodMorph">
    <group>Canines</group>
  </Pawnmorph.MorphDef>

  <Pawnmorph.MorphDef Abstract="True" Name="HerdMorph" ParentName="SensitiveFoodMorph">
    <group>HerbivoreHerding</group>
  </Pawnmorph.MorphDef>
</Defs>