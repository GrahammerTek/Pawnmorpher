<?xml version="1.0" encoding="utf-8"?>

<Defs>

  <Pawnmorph.MorphDef Abstract="True" Name="CanidMorph">
    <categories>
      <li>canid</li>
    </categories>
    <raceSettings>
      <traitSettings>
        <forcedTraits>
          <li>
            <defName>NaturalHunter</defName>
            <chance>100</chance>
          </li>
        </forcedTraits>
      </traitSettings>
    </raceSettings>
  </Pawnmorph.MorphDef>


  <Pawnmorph.MorphDef ParentName="CanidMorph">
    <defName>WolfMorph</defName>
    <label>wolf morph</label>
    <race>Wolf_Timber</race>
  </Pawnmorph.MorphDef>

  <Pawnmorph.MorphDef ParentName="CanidMorph">
    <defName>FoxMorph</defName>
    <label>fox morph</label>
    <race>Fox_Red</race>
    <raceSettings>
      <thoughtSettings>
        <ateAnimalThought>
          <thought>MorphAteAnimalMeatDirect</thought>
          <ingredientThought>MorphAteAnimalMeatAsIngredient</ingredientThought>
        </ateAnimalThought>
      </thoughtSettings>
    </raceSettings>
  </Pawnmorph.MorphDef>


  <Pawnmorph.MorphDef ParentName="CanidMorph">
    <defName>WargMorph</defName>
    <label>warg morph</label>
    <race>Warg</race>
    <raceSettings>
      <thoughtSettings>
        <replacerList>
          <li>
            <original>AteCorpse</original>
            <replacer>WargMorphAteCorpse</replacer>
          </li>
        </replacerList>
        <canEatRaw>true</canEatRaw>
        <suppressHumanlikeCannibalThoughts>true</suppressHumanlikeCannibalThoughts>
      </thoughtSettings>
    </raceSettings>
  </Pawnmorph.MorphDef>


  <Pawnmorph.MorphDef ParentName="CanidMorph">
    <defName>HuskyMorph</defName>
    <label>husky morph</label>
    <race>Husky</race>
  </Pawnmorph.MorphDef>
</Defs>