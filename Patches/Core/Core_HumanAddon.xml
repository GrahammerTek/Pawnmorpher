<Patch>
	<Operation Class="PatchOperationSequence">
		<operations>
			<li Class="PatchOperationTest">
				<xpath>Defs/ThingDef[defName="Human"]</xpath>
				<success>Normal</success>
			</li>
			<li Class="PatchOperationAttributeSet">
				<xpath>Defs/ThingDef[defName="Human"]</xpath>
				<attribute>Class</attribute>
				<value>AlienRace.ThingDef_AlienRace</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/ThingDef[defName="Human"]</xpath>
				<value>
					<alienRace>
						<generalSettings>
							<alienPartGenerator>
								<aliencrowntypes>
									<li>Average_Normal</li>
									<li>Average_Wide</li>
									<li>Average_Pointy</li>
									<li>Narrow_Normal</li>
									<li>Narrow_Pointy</li>
									<li>Narrow_Wide</li>
								</aliencrowntypes>
								<bodyAddons />
							</alienPartGenerator>
						</generalSettings>
					</alienRace>
				</value>
			</li>
			<li Class="PatchOperationAdd">
				<xpath>Defs/BodyDef[defName="Human"]/corePart/parts</xpath>
				<value>
					<li>
						<def>Tail</def>
						<coverage>0</coverage>
						<height>Bottom</height>
						<groups>
							<li>Torso</li>
						</groups>
					</li>
				</value>
			</li>
		</operations>
	</Operation>
</Patch>